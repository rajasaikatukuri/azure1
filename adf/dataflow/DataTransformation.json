{
	"name": "DataTransformation",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_src",
						"type": "DatasetReference"
					},
					"name": "DimAirline"
				},
				{
